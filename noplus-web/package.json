{
  "name": "noplus-web",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "concurrently -k \"npm:dev-*\"",
    "dev-dev": "next dev",
    "build": "conc \"npm:build-*\"",
    "build-build": "next build",
    "start": "conc -k \"npm:start-*\"",
    "lint": "conc \"npm:lint-*\"",
    "start-start": "next start",
    "lint-lint": "next lint",
    "dev-story": "storybook dev -p 6006",
    "build-ghci-story": "storybook build",
    "ghci": "npm ci && conc \"npm:build-ghci-*\" && cd.. && mv noplus-web/storybook-static noplus-web/docs/storybook"
  },
  "dependencies": {
    "@radix-ui/react-slot": "^1.0.2",
    "@sentry/nextjs": "^7.102.0",
    "clsx": "^2.1.0",
    "cva": "npm:class-variance-authority@^0.7.0",
    "next": "^14.1.0",
    "react": "^18",
    "react-dom": "^18",
    "react-feather": "^2.0.10",
    "react-spinners": "^0.13.8",
    "tailwind-merge": "^2.2.1",
    "vaul": "^0.9.0"
  },
  "paths": {
    "@ui/*": [
      "components/ui/*"
    ],
    "@layouts/*": [
      "components/layouts/*"
    ]
  },
  "devDependencies": {
    "@storybook/addon-essentials": "^7.6.17",
    "@storybook/addon-interactions": "^7.6.17",
    "@storybook/addon-links": "^7.6.17",
    "@storybook/addon-onboarding": "^1.0.11",
    "@storybook/blocks": "^7.6.17",
    "@storybook/nextjs": "^7.6.17",
    "@storybook/react": "^7.6.17",
    "@storybook/test": "^7.6.17",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "autoprefixer": "^10.4.17",
    "concurrently": "^8.2.2",
    "eslint": "^8",
    "eslint-config-next": "14.1.0",
    "eslint-plugin-storybook": "^0.8.0",
    "postcss": "^8.4.35",
    "storybook": "^7.6.17",
    "tailwindcss": "^3.4.1",
    "typescript": "^5"
  }
}
