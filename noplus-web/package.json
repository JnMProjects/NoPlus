  "private": true,
  "scripts": {
    "dev": "concurrently -k \"npm run dev-devstart\" \"npm run dev-storybook\"",
    "dev-devstart": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "dev-storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "dependencies": {
    "@sentry/nextjs": "^7.102.0",
    "clsx": "^2.1.0",
    "cva": "npm:class-variance-authority@^0.7.0",
