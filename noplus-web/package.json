{
  "name": "noplus-web",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "concurrently -k \"npm:dev:*\"",
    "dev:next": "next dev",
    "builds": "conc \"npm:build-*\"",
    "build": "next build",
    "start": "conc -k \"npm:start:*\"",
    "lint": "conc \"npm:lint:*\"",
    "start:next": "next start",
    "lint:next": "next lint",
    "dev:storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "build-storydocs": "storybook build --docs",
    "bst": "concurrently \"npm:build:storybook\"",
    "ghci": "npm ci && conc \"npm:ghci:*\"",
    "chromatic": "npx chromatic --storybook-base-dir=noplus-web --zip",
    "test": "conc \"npm:test-*\"",
    "test-jest": "jest",
    "storybook": "storybook dev -p 6006"
  },
  "dependencies": {
    "@radix-ui/react-accordion": "^1.1.2",
    "@radix-ui/react-alert-dialog": "^1.0.5",
    "@radix-ui/react-dialog": "^1.0.5",
    "@radix-ui/react-slot": "^1.0.2",
    "@sentry/nextjs": "^7.102.0",
    "axios": "^1.6.7",
    "clsx": "^2.1.0",
    "cva": "^1.0.0-beta.1",
    "next": "^14.1.0",
    "octokit": "^3.1.2",
    "react": "^18",
    "react-dom": "^18",
    "react-feather": "^2.0.10",
    "react-spinners": "^0.13.8",
    "tailwind-merge": "^2.2.1",
    "vaul": "^0.9.0"
  },
  "paths": {
    "@ui/*": [
      "components/ui/*"
    ],
    "@layouts/*": [
      "components/layouts/*"
    ]
  },
  "devDependencies": {
    "@chromatic-com/storybook": "^1.2.22",
    "@storybook/addon-essentials": "^8.1.0-alpha.1",
    "@storybook/addon-interactions": "^8.1.0-alpha.1",
    "@storybook/addon-links": "^8.1.0-alpha.1",
    "@storybook/addon-styling-webpack": "^0.0.6",
    "@storybook/addon-themes": "^8.1.0-alpha.1",
    "@storybook/blocks": "^8.1.0-alpha.1",
    "@storybook/manager-api": "^8.1.0-alpha.1",
    "@storybook/nextjs": "^8.1.0-alpha.1",
    "@storybook/react": "^8.1.0-alpha.1",
    "@storybook/test": "^8.1.0-alpha.1",
    "@storybook/theming": "^8.1.0-alpha.1",
    "@types/jest": "^29.5.12",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "autoprefixer": "^10.4.17",
    "chromatic": "^11.0.8",
    "concurrently": "^8.2.2",
    "eslint": "^8",
    "eslint-config-next": "14.1.0",
    "eslint-plugin-storybook": "^0.8.0",
    "jest": "^29.7.0",
    "postcss": "^8.4.35",
    "storybook": "^8.1.0-alpha.1",
    "tailwindcss": "^3.4.1",
    "ts-jest": "^29.1.2",
    "tsconfig-paths-webpack-plugin": "^4.1.0",
    "typescript": "^5"
  }
}
